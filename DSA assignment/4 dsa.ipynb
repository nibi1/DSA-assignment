    "repeatation = []\n",
    "def first_non_repeated_char(string):\n",
    "    for i in string:\n",
    "        count = string.count(i)\n",
    "        if count == 1:\n",
    "            repeatation.append(i)\n",
    "    print(repeatation)\n",
    "    return repeatation[0]\n",
    "    \n",
    "string = input(\"enter a string: \")\n",
    "first_non_repeated_char(string)"
